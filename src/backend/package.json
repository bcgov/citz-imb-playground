{
  "name": "playground-express",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "main": "index.ts",
  "scripts": {
    "dev": "nodemon -L --exec ts-node index.ts",
    "build": "tsc",
    "test": "node --import tsx --test src/**/*.test.*",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js ",
    "migration:generate": "npm run typeorm -- migration:generate -d dataSource.ts",
    "migration:run": "npm run build && npm run typeorm -- migration:run -d dataSource.ts",
    "migration:run:no-build": "npm run typeorm -- migration:run -d dataSource.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d dataSource.ts",
    "migration:create": "npm run typeorm -- migration:create"
  },
  "dependencies": {
    "@bcgov/citz-imb-kc-css-api": "https://github.com/bcgov/citz-imb-kc-css-api/releases/download/v1.3.1/bcgov-citz-imb-kc-css-api-1.3.1.tgz",
    "@bcgov/citz-imb-kc-express": "https://github.com/bcgov/citz-imb-kc-express/releases/download/v1.3.1/bcgov-citz-imb-kc-express-1.3.1.tgz",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "express": "4.18.2",
    "express-rate-limit": "7.0.1",
    "pg": "8.11.3",
    "reflect-metadata": "0.1.13",
    "rss-parser": "3.13.0",
    "typeorm": "0.3.17",
    "typescript": "5.2.2",
    "zod": "3.22.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "1.4.4",
    "@types/cors": "2.8.14",
    "@types/express": "4.17.17",
    "@types/node": "20.6.3",
    "@types/pg": "8.10.2",
    "@typescript-eslint/eslint-plugin": "6.7.2",
    "@typescript-eslint/parser": "6.7.2",
    "eslint": "8.49.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "nodemon": "3.0.1",
    "prettier": "3.0.3",
    "ts-node": "10.9.1",
    "tsx": "3.14.0"
  }
}
